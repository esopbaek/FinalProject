<div class="all-tables group">
  <div class="left">
  <div class="nutrition group">
  <table class="nutrition-goals">
    <tr>
      <th>Nutrition Goals</th>
      <th>Goals</th>
    </tr>

    <tr>

      <td><strong>Net Calories Consumed*/Day</strong></td>
      <td class="red"><%= dietProfile.get("netCal") %> Calories/Day</td>
    </tr>

    <tr>
      <td>Carbs/Day</td>
      <td><%= dietProfile.get("carbs") %> g</td>
    </tr>

    <tr>
      <td>Fat/Day</td>
      <td><%= dietProfile.get("fats") %> g</td>
    </tr>

    <tr>
      <td>Protein/Day</td>
      <td><%= dietProfile.get("protein") %> g</td>
    </tr>
  </table>
  </div>


  <div class="fitness">
  <table class="fitness-goals">
    <tr>
      <th>Fitness Goals</th>
      <th>Goals</th>
    </tr>

    <tr>
      <td><strong>Calories Burned/Week</strong></td>
      <td class="red"><%= dietProfile.get("calsBurned") %> Calories / Week</td>
    </tr>

    <tr>
      <td>Workouts/Week</td>
      <td><%= dietProfile.get("workouts_per_week") %> workouts</td>
    </tr>

    <tr>
      <td>Minutes/Workout</td>
      <td><%= dietProfile.get("mins_per_workout") %> minutes</td>
    </tr>
  </table>
  </div>
  </div>
  <div class="right">
  <table class="diet-profile">
    <tr>
      <th>Your Diet Profile</th>
      <th>Target</th>
    </tr>

    <tr>
      <td colspan="2"><strong>Calories Burned</strong></td>
    </tr>

    <tr>
      <td>From Normal Daily Activity</td>
      <td><%= dietProfile.get("bmr") %> cal/day</td>
    </tr>

    <tr>
      <td colspan="2"><strong>Net Calories Consumed*</strong></td>
    </tr>

    <tr>
      <td>Your Daily Goal</td>
      <td><%= dietProfile.get("netCal") %> cal/day</td>
    </tr>

    <tr>
      <td>Daily Calorie Defecit</td>
      <td class="red"><%= dietProfile.get("calDeficit") %> calories</td>
    </tr>

    <tr>
      <td>Projected Weight Loss</td>
      <td class="red"><%= dietProfile.get("projectedLoss") %></td>
    </tr>
  </table>

  </div>
</div>

